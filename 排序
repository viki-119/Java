冒泡排序：
public class bubbleSort {  
	public static void main(String[] args){
	    int a[]={49,38,65,97,76,13,27,49};  
	    int temp=0;  
	    for(int i=0;i<a.length-1;i++) { 
	        for(int j=0;j<a.length-1-i;j++){
		        if(a[j]>a[j+1]){
		            temp=a[j];  
		            a[j]=a[j+1];  
		            a[j+1]=temp;  
		        }  
	        }  
	    }  
	    for(int i=0;i<a.length;i++) {
	    	System.out.println(a[i]);     
	    }
	}  	
}  
